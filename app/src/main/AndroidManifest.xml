<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.packtpub.memorygame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Game1Level1Activity" />
        <activity android:name=".Game1Level2Activity" />
        <activity android:name=".Game1Level3Activity"/>
        <activity android:name=".Game2Level1Activity" />
        <activity android:name=".Game2Level2Activity" />
        <activity android:name=".Game2Level3Activity"/>
        <activity android:name=".RulesActivity" />
        <activity android:name=".ResultsActivity" />

    </application>

</manifest>